# This is an example environment file. Copy it to .env and fill in your actual values.

# RPC URLs for different chains
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Block Explorer URLs (Testnets)
ARBITRUM_SEPOLIA_EXPLORER_URL=https://arbitrum-sepolia.blockscout.com

#### Wallets for different use cases ##################################
# If you lack addresses, can assign same address multiple times 

# Deployer needs to own native tokens. Will also be owner of all deployed contracts
DEPLOYMENT_WALLET=0x
# Input the same key twice
DEPLOYMENT_KEY=0x
DEPLOYMENT_PRIVATE_KEY=0x

# Pool creator needs native tokens for gas and should have USDC for initial liquidity
POOL_CREATION_WALLET=0x
POOL_CREATION_PRIVATE_KEY=0x

# Swapper needs tokens for both sides of the pool
SWAPPER_WALLET=0x
SWAPPER_PRIVATE_KEY=0x

# Liquidity provider needs tokens for both sides of the pool
LIQUIDITY_PROVIDER_WALLET=0x
LIQUIDITY_PROVIDER_PRIVATE_KEY=0x

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# SyncroHook deployment address (set after deploying the hook)
SYNCRO_HOOK_ADDRESS=0x0000000000000000000000000000000000000000

